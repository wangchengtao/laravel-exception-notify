{
    "name": "hicoopay/laravel-exception-notify",
    "description": "laravel 框架适配异常监控通知, 支持钉钉群机器人, 飞书群机器人以及自定义通道",
    "keywords": [
        "laravel", "exception", "notify", "dingtalk", "feishu"
    ],
    "authors": [
        {
            "name": "wangchengtao",
            "email": "915129420@qq.com"
        }
    ],
    "require": {
        "php": ">=7.2",
        "hicoopay/exception-notify": "dev-master",
        "illuminate/support": "^6.0 || ^7.0 || ^8.0 || ^9.0 || ^10.0"
    },
    "autoload": {
        "psr-4": {
            "Hicoopay\\LaravelExceptionNotify\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Hicoopay\\LaravelExceptionNotify\\ExceptionNotifyServiceProvider"
            ]
        }
    },
    "config": {
        "secure-http": false
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "http://gitlab.mayihaike.xyz:9091/hicoopay/exception-notify.git"
        }
    ]
}
