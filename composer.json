{
    "name": "summer/laravel-exception-notify",
    "description": "laravel 框架适配异常监控通知, 支持钉钉群机器人, 飞书群机器人以及自定义通道",
    "keywords": [
        "laravel", "exception", "notify", "dingtalk", "feishu"
    ],
    "authors": [
        {
            "name": "wangchengtao",
            "email": "915129420@qq.com"
        }
    ],
    "require": {
        "php": ">=7.2",
        "summer/exception-notify": "dev-master",
        "illuminate/support": "^6.0 || ^7.0 || ^8.0 || ^9.0 || ^10.0"
    },
    "autoload": {
        "psr-4": {
            "Summer\\LaravelExceptionNotify\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Summer\\LaravelExceptionNotify\\ExceptionNotifyServiceProvider"
            ]
        }
    }
}
